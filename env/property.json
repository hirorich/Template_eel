{
    "app_name": "FFCaller",
    "system_date": "YYYY/mm/dd",
    "system_time": "HH:MM:SS",
    
    "add_data": {
        "cmn_db_sqlite3": "./db/cmn_db.sqlite3",
        "ffc_db_sqlite3": "./db/ffc_db.sqlite3",
        "icon_ico": "./icon/icon.ico"
    },
    
    "eel": {
        "init": "web",
        "start": "index.html",
        "mode": "chrome",
        "port": 0,
        "position": [400, 100],
        "size": [800, 600],
        "cmdline_args": ["--incognito"]
    },
    
    "external_cmd": {
        "ffmpeg": "ffmpeg",
        "ffprobe": "ffprobe"
    },
    
    "workdir":"media",
    "outdir":"output",
    "target_extension": ["mp4", "mp3"]
}
